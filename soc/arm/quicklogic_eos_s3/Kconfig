config SOC_EOS_S3
	bool

config SOC_EOS_S3_GPIO
       bool
       help
         Set if the General Purpose Input/Output (GPIO) HAL module is used.

config SOC_EOS_S3_FPGA
       bool
       help
         Enable on chip FPGA

config EOS_S3_PROGRAM_FPGA
	bool
	help
	  Enable programming the FPGA IP on EOS S3

